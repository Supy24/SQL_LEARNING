SELECT * FROM SALARYWINDOW

---------------------------------- WINDOW FUNCTION

SELECT NAME , DEPT , SUM(SALARY) 
OVER() AS TOTALSALARY
FROM SALARYWINDOW


SELECT NAME , DEPT , MIN(SALARY) 
OVER() AS TOTALSALARY
FROM SALARYWINDOW

------------ PARTION BY 

SELECT NAME , DEPT ,SUM(SALARY) 
OVER(PARTITION BY DEPT) AS TOTALSALARY
FROM SALARYWINDOW
------------------------------ RANK FUNCTION


SELECT NAME , DEPT ,SALARY,
RANK() OVER( ORDER BY SALARY DESC) AS RANK
FROM SALARYWINDOW


SELECT NAME , DEPT ,SALARY,
RANK() OVER( PARTITION BY DEPT ORDER BY SALARY DESC) AS RANK
FROM SALARYWINDOW


-----------------------------DEBSE_RANK()


SELECT NAME , DEPT ,SALARY,
DENSE_RANK() OVER( ORDER BY SALARY DESC) AS RANK
FROM SALARYWINDOW

SELECT NAME , DEPT ,SALARY,
DENSE_RANK() OVER( PARTITION BY DEPT ORDER BY SALARY DESC) AS RANK
FROM SALARYWINDOW

-------------------------ROW_NUBER

SELECT NAME , DEPT ,SALARY,
ROW_NUMBER() OVER( ORDER BY SALARY DESC) AS RANK
FROM SALARYWINDOW

SELECT NAME , DEPT ,SALARY,
RANK() OVER( PARTITION BY DEPT ORDER BY SALARY DESC) AS RANK,
DENSE_RANK() OVER( PARTITION BY DEPT ORDER BY SALARY DESC) AS DRANK,
ROW_NUMBER() OVER( PARTITION BY DEPT  ORDER BY SALARY DESC) AS RRANK
FROM SALARYWINDOW



SELECT NAME , DEPT ,SALARY,
ROW_NUMBER() OVER( PARTITION BY DEPT  ORDER BY SALARY DESC) AS RANK
FROM SALARYWINDOW
